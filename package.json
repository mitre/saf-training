  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.24",
    "cypress": "^15.2.0",
    "cypress-each": "^1.14.1",
    "flowchart.ts": "^3.0.1",
    "mermaid": "^11.11.0",
    "start-server-and-test": "^2.1.1",
    "vue": "^3.5.21",
    "vuepress": "2.0.0-rc.24",
    "vuepress-theme-hope": "2.0.0-rc.94"
  }
}
