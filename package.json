{
  "name": "saf-training",
  "version": "2.0.0",
  "description": "Training site for the MITRE Security Automation Framework",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "docs:build": "vitepress build src",
    "docs:dev": "vitepress dev src",
    "docs:preview": "vitepress preview src",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "pdf:all": "npx press-export-pdf export src --config vitepress-pdf.config.ts",
    "pdf:getting-started": "npx press-export-pdf export src --config pdf-configs/Getting-Started-SAF-User-Class.config.ts",
    "pdf:beginner": "npx press-export-pdf export src --config pdf-configs/InSpec-Training-Beginner.config.ts",
    "pdf:advanced": "npx press-export-pdf export src --config pdf-configs/InSpec-Training-Advanced.config.ts",
    "pdf:profile-dev": "npx press-export-pdf export src --config pdf-configs/InSpec-Profile-Development.config.ts",
    "pdf:stig": "npx press-export-pdf export src --config pdf-configs/STIG-Development.config.ts",
    "pdf:delta": "npx press-export-pdf export src --config pdf-configs/SAF-Delta.config.ts",
    "pdf:mappers": "npx press-export-pdf export src --config pdf-configs/OHDF-Mapper-Development.config.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "flowchart.ts": "^3.0.1",
    "medium-zoom": "^1.1.0",
    "mermaid": "^11.12.2",
    "vitepress": "^1.6.4",
    "vitepress-export-pdf": "^1.0.0",
    "vitepress-plugin-comment-with-giscus": "^1.1.15",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitepress-plugin-tabs": "^0.7.3",
    "vitepress-sidebar": "^1.33.1",
    "vue": "^3.5.25"
  }
}
